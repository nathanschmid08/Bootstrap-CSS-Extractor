{
    "manifest_version": 3,
    "name": "Bootstrap CSS Extractor",
    "version": "1.0",
    "description": "Extrahiert das Bootstrap CDN CSS und speichert es als Datei.",
    "permissions": [
        "downloads",
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": []
}